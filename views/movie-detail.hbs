<a href="/movies">Go Back</a>
<div class="main-movie">
    <div class="left-container">
        <img src={{movie.image}}>
    </div>
    <div class="right-container">
        <h2>{{movie.title}}</h2>
        <h3>Director: {{movie.director}}</h3>
            STARS:
        {{#each movie.stars}}
        <ul>
            <li>{{this}}</li>
        </ul>
        {{/each}}
        <p>{{movie.description}}</p>
        {{#each movie.showtimes}}
        <ul class="times-stamps">
            <li>{{this}}</li>
        </ul>
        {{/each}}
    </div>
</div>